{
  "name": "ai-assistant-full",
  "version": "1.0.0",
  "description": "AI Assistant web app with Supabase",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "build": "node scripts/build.js",
    "cap:sync": "npm run build && npx cap sync",
    "cap:android": "npm run cap:sync && npx cap open android",
    "cap:ios": "npm run cap:sync && npx cap open ios",
    "cap:run:android": "npm run cap:sync && npx cap run android",
    "cap:run:ios": "npm run cap:sync && npx cap run ios"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.1",
    "@capacitor/app": "^8.0.0",
    "@capacitor/camera": "^8.0.0",
    "@capacitor/clipboard": "^8.0.0",
    "@capacitor/device": "^8.0.0",
    "@capacitor/filesystem": "^8.1.0",
    "@capacitor/geolocation": "^8.0.0",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/ios": "^8.0.1",
    "@capacitor/local-notifications": "^8.0.0",
    "@capacitor/share": "^8.0.0",
    "@capgo/capacitor-contacts": "^8.0.5",
    "@coinbase/wallet-sdk": "^4.3.7",
    "@ebarooni/capacitor-calendar": "^8.0.0",
    "@google/generative-ai": "^0.24.1",
    "@supabase/supabase-js": "^2.39.7",
    "animejs": "^4.3.0",
    "ethers": "^6.16.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1"
  }
}
