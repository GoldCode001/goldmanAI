{
  "productName": "Goldman AI",
  "version": "1.0.0",
  "identifier": "com.goldmanai.app",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devUrl": "https://goldmanai-production.up.railway.app",
    "frontendDist": "../www"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Goldman AI",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "transparent": false,
        "decorations": true,
        "alwaysOnTop": false,
        "visible": false,
        "url": "loader.html"
      },
      {
        "label": "bubble",
        "title": "PAL",
        "width": 120,
        "height": 120,
        "resizable": false,
        "fullscreen": false,
        "transparent": true,
        "decorations": false,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "visible": true,
        "url": "loader-bubble.html"
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["remote-api-access"]
    }
  },
  "plugins": {}
}
